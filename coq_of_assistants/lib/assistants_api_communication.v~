  Definition delete_thread_handler
      (__arg0 : axum.extract.path.Path.t alloc.string.String.t)
      (__arg1
        :
        axum.extract.state.State.t
          assistants_api_communication.models.AppState.t)
      : M OpaqueDef :=
    let* __arg0 := M.alloc __arg0 in
