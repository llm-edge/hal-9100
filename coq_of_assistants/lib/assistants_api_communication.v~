      Path((thread_id,)): Path<(String,)>,
      Query(pagination_params): Query<ListMessagePaginationParams>,
      State(app_state): State<AppState>,
  ) -> Result<JsonResponse<ListMessagesResponse>, (StatusCode, String)> {
      // let PaginationParams {
      //     limit,
      //     order,
      //     after,
      //     before,
      // } = pagination_params;
